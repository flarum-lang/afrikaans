{
	"name": "flarum-lang/afrikaans",
	"type": "flarum-extension",
	"description": "Afrikaans language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/afrikaans",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"af-locale",
		"afrikaans-locale",
		"afrikaans"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/afrikaans/issues",
		"source": "https://github.com/flarum-lang/afrikaans"
	},
	"require": {
		"flarum/core": "^1.6"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Afrikaans",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "af",
			"title": "Afrikaans"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
